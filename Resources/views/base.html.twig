<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>SB Admin 2 - Bootstrap Admin Theme</title>

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

    {% block stylesheets %}
        {% stylesheets
            '@ZanStandardUiBundle/Resources/public/css/bootstrap.min.css'
            '@ZanStandardUiBundle/Resources/public/css/metisMenu.min.css'
            '@ZanStandardUiBundle/Resources/public/css/sb-admin-2.css'
            'bundles/zanstandardui/font-awesome/css/font-awesome.min.css'
        output='css/*.css' filter='cssrewrite' %}

        <link rel="stylesheet" type="text/css" href="{{ asset_url }}" />
        {% endstylesheets %}

    {% endblock %}

</head>

<body>
    <div id="wrapper">
        {% block body %}{% endblock %}
    </div>

    {% block javascripts %}
        {% javascripts
            '@ZanStandardUiBundle/Resources/public/js/jquery.min.js'
            '@ZanStandardUiBundle/Resources/public/js/bootstrap.min.js'
            '@ZanStandardUiBundle/Resources/public/js/metisMenu.min.js'
            '@ZanStandardUiBundle/Resources/public/js/sb-admin-2.js'
        %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}

    {# Bock for custom scripts in child page #}
    {% block scripts %}{% endblock %}
</body>
</html>